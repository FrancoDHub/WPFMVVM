<Window x:Class="WPFMVVM.ViewInterfaces.DataEntry.AbsenceDataEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFMVVM.ViewInterfaces.DataEntry"
        mc:Ignorable="d"
        Title="AbsenceDataEntry" Height="350" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="263*"/>
            <RowDefinition Height="57*"/>
        </Grid.RowDefinitions>
        <Label Content="IdAbsence" HorizontalAlignment="Left" Margin="31,48,0,0" VerticalAlignment="Top" Width="170"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="251,51,0,0" TextWrapping="Wrap" Text="{Binding IdAbsence}" VerticalAlignment="Top" Width="203"/>
        <Button x:Name="btnEnreistrer" Content="Enregistrer" HorizontalAlignment="Left" Margin="107,18,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.238,0.021"/>
        <Button x:Name="btnAnnuler" Content="Annuler" HorizontalAlignment="Left" Margin="335,18,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.238,0.021"/>

    </Grid>
</Window>
